{
	"name": "figma-sync",
	"displayName": "figma-sync",
	"description": "Tools to keep Figma designs and front-end code in sync",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.figmaToLess"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.figmaToLess",
				"title": "Generate LESS file from Figma"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Figma Sync",
			"properties": {
				"figmaSync.APIKey": {
					"type": "string",
					"default": "",
					"description": "Your Figma API key"
				},
				"figmaSync.fileKey": {
					"type": "string",
					"default": "",
					"description": "The key for the file containing your design system"
				},
				"figmaSync.outFileName": {
					"type": "string",
					"default": "",
					"description": "(optional) The name of the file that will be generated."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"figma-js": "^1.6.1"
	}
}
